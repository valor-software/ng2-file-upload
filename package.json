    "cpy-cli": "1.0.1",
    "del-cli": "0.2.0",
    "es6-promise": "4.0.5",
    "es6-shim": "0.35.1",
    "es7-reflect-metadata": "1.6.0",
    "eslint-config-valorsoft": "0.1.0",
    "gh-pages": "0.11.0",
    "gitignore-to-glob": "0.2.1",
    "gulp": "3.9.1",
    "gulp-size": "2.1.0",
    "gulp-tslint": "6.1.2",
    "lite-server": "2.2.2",
    "marked": "0.3.6",
    "ng2-bootstrap": "1.1.14",
    "ng2-webpack-config": "0.0.5",
    "pre-commit": "1.1.3",
    "prismjs": "1.5.1",
    "prismjs-loader": "0.0.4",
    "reflect-metadata": "0.1.8",
    "require-dir": "0.3.1",
    "rxjs": "5.0.0-beta.12",
    "systemjs-builder": "0.15.32",
    "tslint-config-valorsoft": "1.1.1",
    "typescript": "2.0.3",
    "zone.js": "0.6.26"
  },
