{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/ng2-file-upload",
    "target": "es2015",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "lib": ["dom", "es2018"]
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableResourceInlining": true
  },
  "exclude": [
    "**/*.spec.ts"],
  "include": ["**/*.ts"]
}
